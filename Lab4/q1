# Python program to remove empty tuples from a List
# Input : tuples = [(), (‘r’,’15’,’8′), (), (‘l’, ‘’), (‘k’, ‘a’, ’45’), (”,”),()]
# Output : [(‘r’, ’15’, ‘8’), (‘l’, ‘s’), (‘k’, ‘a’, ’45’), (”, ”)]

def remove(tuples):
    return [t for t in tuples if t]

tuples = [(), ('r', '15', '8'), (), ('l', 's'), ('k', 'a', '45'), ('', ''), ()]
result = remove(tuples)
print("Input:", tuples)
print("Output:", result)
